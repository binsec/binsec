module type S = sig
  type 'a t

  val push : 'a -> 'a t -> 'a t
  val pop : 'a t -> 'a * 'a t
  val singleton : 'a -> 'a t
  val length : 'a t -> int
  val is_empty : 'a t -> bool
  val empty : 'a t
end

module Dfs : S
module Bfs : S
module Nurs : S
