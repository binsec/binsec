(include_subdirs no)

(library
 (public_name binsec.cli.xtrasec)
 (name binsec_cli_xtrasec)
 (modules :standard)
 (private_modules llvm_decoder)
 (flags
  (:standard -open Binsec -open Binsec_isa_x86_32 -w -69))
 (library_flags
  (:standard -linkall))
 (libraries
  binsec
  binsec.isa.x86_32
  (select
   llvm_decoder.ml
   from
   (llvm -> llvm_decoder.enabled.ml)
   (!llvm -> llvm_decoder.disabled.ml))))
